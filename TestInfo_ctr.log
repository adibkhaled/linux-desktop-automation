int TComTask::SendReply() const
{
    VIC_ASSERT(m_internal_simpl_sender_data != nullptr);
    if (m_internal_simpl_sender_data == nullptr)
    {
        fmt::println(stderr, "protocol error: SendReply called by '{}' but Receive() did not get data?", my_name_as_string());
    }
    auto result = Reply(m_internal_simpl_sender_data, nullptr, 0);
    m_internal_simpl_sender_data = nullptr;
    return result;
}

int TComTask::SendReply(const TBufTask & buffer) const
{
    VIC_ASSERT(m_internal_simpl_sender_data != nullptr);

    if (m_internal_simpl_sender_data == nullptr)
    {
        fmt::println(stderr, "protocol error: SendReply called by '{}' but Receive() did not get data? command: {}, buffer: {}", my_name_as_string(), GetCommand(buffer), as_hexdump(buffer));
    }

    comtaskPrint("  [SendReply {0:20s}] {1}\n", sendMarker(), as_hexdump(buffer));
    auto result = Reply(m_internal_simpl_sender_data, const_cast<char *>(buffer.data()), buffer.size());
    m_internal_simpl_sender_data = nullptr;
    return result;
}
